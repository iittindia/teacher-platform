# Database
DATABASE_URL="postgresql://user:password@localhost:5432/your_database_name"

# Next Auth
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="your_nextauth_secret_here"

# OpenAI
OPENAI_API_KEY="your_openai_api_key_here"

# Resend
